ns_set="cef5dd"

if [ "$1" = "dev" ]; then
    env="dev"
elif [ "$1" = "test" ]; then
    env="test"
elif [ "$1" = "prod" ]; then
    env="prod"
else
    env=""
fi

if [ "$env" = "" ]; then
   echo "Error.  Invalid environment specified: $1\n"
   return 1
else
   ns="$ns_set-$env"
fi

export TITLEPREFIX="Mock Splunk App $ns "

echo -ne "\e]0;Mock Splunk App $ns\a"
kamel -n $ns log mock-splunk-app
