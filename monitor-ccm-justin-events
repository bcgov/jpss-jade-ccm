ns_set="cef5dd"

if [ "$1" = "dev" ]; then
    env="dev"
elif [ "$1" = "test" ]; then
    env="test"
else
    env=""
fi

if [ "$env" = "" ]; then
   echo "Error.  Invalid environment specified: $1\n"
   return 1
else
   ns="$ns_set-$env"
fi

echo "`basename $0` to $ns ..."

export TITLEPREFIX="JUSTIN Events $ns "
echo -ne "\e]0;JUSTIN Adapter $ns\a"

kamel -n $ns log ccm-justin-adapter | grep -v "Event batch count: 0" | grep "Event batch "
