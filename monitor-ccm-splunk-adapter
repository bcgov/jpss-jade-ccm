ns_set="cef5dd"

if [ "$1" = "dev" ]; then
    env="dev"
elif [ "$1" = "test" ]; then
    env="test"
else
    env=""
fi

if [ "$env" = "" ]; then
   echo "Error.  Invalid environment specified: $1\n"
   return 1
else
   ns="$ns_set-$env"
fi

echo "`basename $0` to $ns ..."

export TITLEPREFIX="SPLUNK Adapter   "
echo -ne "\e]0;SPLUNK Adapter\a"

kamel -n $ns log ccm-splunk-adapter
