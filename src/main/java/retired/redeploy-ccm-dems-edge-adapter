kamel -n cef5dd-dev delete ccm-dems-edge-adapter
kamel -n cef5dd-dev run CcmDemsEdgeAdapter/CcmDemsEdgeAdapter.java \
  --property file:CcmDemsEdgeAdapter/application.properties \
  --config secret:dems-secrets \
  --config secret:dems-edge-adapter-secrets \
  --profile openshift  \
  --trait service.node-port=false \
  --trait route.tls-termination=edge \
  --trait route.tls-insecure-edge-termination-policy=Redirect \
  --trait route.host=ccm-dems-edge-adapter-dev.apps.silver.devops.gov.bc.ca
