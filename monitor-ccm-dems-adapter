ns_set="cef5dd"

if [ "$1" = "dev" ]; then
    env="dev"
elif [ "$1" = "test" ]; then
    env="test"
else
    env=""
fi

if [ "$env" = "" ]; then
   echo "Error.  Invalid environment specified: $1\n"
   return 1
else
   ns="$ns_set-$env"
fi

export TITLEPREFIX="DEMS Adapter $ns "

echo -ne "\e]0;DEMS Adapter $ns\a"
kamel -n $ns log ccm-dems-adapter
